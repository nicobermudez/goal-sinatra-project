<html>
<head>
  <title>Feed</title>
  <style>
    .hero {
      padding: 40px;
    }
  </style>
</head>


  <header>
    <a href ="/"><h1>GOAL</h1></a>
    <nav>
      <a href="/home">Home</a>
      <a href="/feed">Feed</a>
      <a href="/">Log Out</a>
    </nav>
  </header>
  <body>
    <section class="hero">
      <% Goal.all.each do |goal| %>
        <% user = User.find_by_id(goal.user_id)%>
        <% if user %>
          <p> <%= user.username %>: <%=goal.title%></p>
          <% end %>
      <% end %>
    </section>
  </body>
</html>
